/dts-v1/;

/include/ "bcm7425.dtsi"

/ {
	compatible = "brcm,bcm97425svmb", "brcm,bcm7425";
	model = "Broadcom BCM97425SVMB";

	memory@0 {
		device_type = "memory";
		reg = <0x00000000 0x10000000>,
		      <0x20000000 0x30000000>,
		      <0x90000000 0x40000000>;
	};

	chosen {
		bootargs = "console=ttyS0,115200";
		stdout-path = &uart0;
	};
};

&uart0 {
	status = "okay";
};

&enet0 {
	status = "okay";
};

&enet1 {
	status = "okay";
};

&moca {
	status = "okay";
};

&ehci0 {
	status = "okay";
};

&ohci0 {
	status = "okay";
};

&ehci1 {
	status = "okay";
};

&ohci1 {
	status = "okay";
};

&ehci2 {
	status = "okay";
};

&ohci2 {
	status = "okay";
};

&ehci3 {
	status = "okay";
};

&ohci3 {
	status = "okay";
};

&sata {
	status = "okay";
};

&sata_phy {
	status = "okay";
};

&nand {
	status = "okay";

	nandcs@1 {
		#size-cells = <0x2>;
		#address-cells = <0x2>;
		compatible = "brcm,nandcs";
		reg = <0x1>;
		nand-on-flash-bbt;

		nand-ecc-strength = <24>;
		nand-ecc-step-size = <1024>;
		brcm,nand-oob-sector-size = <27>;

		rootfs@0 {
			reg = <0x0 0x0 0x0 0x0>;
		};

		kernel@1 {
		        reg = <0x0 0x0 0x0 0x0>;
		};

		flash@0 {
			reg = <0x0 0x0 0x1 0x0>;
		};
	};
};

&pcie {
	status = "okay";
};
